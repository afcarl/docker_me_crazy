<p id="notice"><%= notice %></p>

<h1>Launch</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @launch.each do |launch| %>
      <tr>
        <td><%= link_to 'Show', launch %></td>
        <td><%= link_to 'Edit', edit_launch_path({launch: launch.id}) %></td>
        <td><%= link_to 'Destroy', launch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Launch', new_launch_path %>
